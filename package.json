{
  "name": "hakathon-speech-recognition",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "tsc": "tsc --noEmit",
    "test": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test:l": "LOG_LEVEL=info npm t",
    "check": "ts-node scripts/check",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "deploy": "npm run lint && npm t && npm run build && deploy-fn"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts --fix"
  },
  "dependencies": {
    "@google-cloud/speech": "^4.9.0",
    "@google-cloud/storage": "^5.15.3",
    "az": "^0.2.3",
    "console-log-level": "^1.4.1",
    "content-disposition": "^0.5.3",
    "docx": "^7.1.1"
  },
  "devDependencies": {
    "@types/console-log-level": "^1.4.2",
    "@types/content-disposition": "^0.5.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.4",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.2.4",
    "mocha": "^9.1.3",
    "node-fetch": "^2.6.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "yandex-cloud-deploy-fn": "^1.1.3"
  },
  "license": "MIT"
}
